@article{hedley98:techn:diffus:cultur:imper,
	Author = {Hedley, R. Alan},
	Date-Added = {2008-10-25},
	Date-Modified = {2008-10-31 11:51:18 -0700},
	Journal = {International Journal of Comparative Sociology},
	Number = {2},
	Optnote = {June},
	Pages = {198--212},
	Title = {Technological Diffusion or Cultural Imperialism? {M}easuring the Information Revolution},
	Volume = {39},
	Year = {1998},
	Abstract = {Conceptualizes the multidimensional term
		  informationalization and measures its various dimensions
		  among the G-7 nations, using Organization for Economic
		  Cooperation and Development data. Three major results flow
		  from the analysis: (1) The information revolution is still
		  very much in its beginning stages. Few workers are employed
		  directly in computing, and revenues from the industry
		  contribute only marginally to each country's economy.
		  Although there is a sizeable information technology (it)
		  infrastructure in place, just a small fraction of the
		  general populace actually uses computers, and fewer are
		  connected to the Internet. (2) The information revolution
		  is largely concentrated in the g-7 countries: only 5 of
		  them account for 80\% of the it market. (3) Analysis of the
		  Internet reveals that it is overwhelmingly US-based,
		  English-speaking, and Western-focused. Thus, the
		  information revolution could presage the rapid and
		  widespread transmission of information in a culture-laden,
		  asymmetrical way. A call is made for international dialogue
		  on how it could be introduced in culturally harmonious
		  ways. 7 Tables, 24 References. Adapted from the source
		  document}}

