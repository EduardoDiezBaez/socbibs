@article{ervin99:travel:super:highw,
	Author = {Kelly S. Ervin and Geoff Gilmore},
	Date-Added = {2008-10-25},
	Date-Modified = {2008-10-31 11:51:19 -0700},
	Journal = {Journal of Black Studies},
	Optnote = {Jan},
	Optnumber = {3},
	Pages = {398--407},
	Title = {Traveling the Superinformation Highway: African Americans' Perceptions and Use of Cyberspace Technology},
	Volume = {29},
	Year = {1999},
	Abstract = {Investigates the usage levels and perceptions of the
		  Internet, the Web, and computers by African American,
		  Hispanic, Asian Pacific American, and Native American
		  college students, drawing on focus group and survey data (n
		  = 247). It is found that African American students used the
		  Internet, the Web, and computers as much as other students
		  did. The primary determinant of usage was whether a student
		  owned a computer. However, even in this case, African
		  Americans who did not own a computer used computers more
		  often than whites who were similarly situated. Thus, recent
		  commentary suggesting that a racial divide exists in
		  computer usage, caused by gaps in income levels, is
		  rejected. Instead, it is argued that, if African Americans
		  use computers less often than whites in the general
		  population, this may be due to a lack of trust among
		  African Americans toward an invasive new technology. 1
		  Table, 7 References. D. Ryfe}}

@article{esler98:evolut,
	Author = {Esler, S. L. and Nelson, M. L.},
	Date-Added = {2008-10-25},
	Date-Modified = {2008-10-31 11:51:17 -0700},
	Journal = {Journal of the American Society For Information Science},
	Optnote = {JAN J AMER SOC INFORM SCI},
	Optnumber = {1},
	Pages = {82--91},
	Title = {Evolution of scientific and technical information distribution},
	Volume = {49},
	Year = {1998},
	Abstract = {World Wide Web (WWW) and related information technologies
		  are transforming the distribution of scientific and
		  technical information (STI), We examine 11 recent,
		  functioning digital libraries focusing on the distribution
		  of STI publications, including journal articles, conference
		  papers, and technical reports. We introduce 4 main
		  categories of digital library projects: Based on the
		  architecture (distributed vs, centralized) and the
		  contributor (traditional publisher vs. authoring
		  individual/organization). Many digital library prototypes
		  merely automate existing publishing practices or focus
		  solely on the digitization of the publishing cycle output,
		  not sampling and capturing elements of the input. Still
		  others do not consider for distribution the large body of
		  'gray literature.' We address these deficiencies in the
		  current model of STI exchange by suggesting methods for
		  expanding the scope and target of digital libraries by
		  focusing on a greater source of technical publications and
		  using 'buckets,' an object- oriented construct for grouping
		  logically related information objects, to include holdings
		  other than technical publications.}}

