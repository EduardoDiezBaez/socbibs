@article{marx99:name,
	Author = {Marx, Gary T.},
	Date-Added = {2008-10-25},
	Date-Modified = {2008-10-31 11:51:19 -0700},
	Journal = {Information Society},
	Number = {2},
	Optnote = {Apr-June},
	Pages = {99--112},
	Title = {What's in a Name? Some Reflections on the Sociology of Anonymity},
	Volume = {15},
	Year = {1999},
	Abstract = {Aims to lay out some of the conceptual landscape needed to
		  better understand anonymity and identifiability in
		  contemporary life. Seven types of identity knowledge are
		  suggested, involving legal name, location, symbols linked
		  and not linked back to these through intermediaries,
		  distinctive appearance and behavior patterns, social
		  categorization, and certification via knowledge or
		  artifacts. A number of major rationales and contexts for
		  anonymity (free flow of communication, protection,
		  experimentation) and identifiability (eg, accountability,
		  reciprocity, eligibility) are identified, and a principle
		  of 'truth in the nature of naming,' which holds that those
		  who use pseudonyms on the Internet in personal
		  communications have an obligation to indicate they are
		  doing so, is suggested. In an effort to guide the
		  development and assessment of any Internet policy regarding
		  anonymity, 13 procedural questions are offered. 3 Tables,
		  28 References. Adapted from the source document}}

