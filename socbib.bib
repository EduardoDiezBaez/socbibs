@article{dean99:virtual:fears,
	Author = {Dean, Jodi},
	Date-Added = {2008-10-25},
	Date-Modified = {2008-10-31 11:51:19 -0700},
	Journal = {Signs},
	Optnote = {Summer},
	Optnumber = {4},
	Pages = {1169--1078},
	Title = {Virtual Fears},
	Volume = {24},
	Year = {1999},
	Abstract = {Examines fears related to the Internet that have prompted
		  demands for regulation on behalf of 'vulnerable' women and
		  children. It is argued that frightening misconceptions have
		  prevented many women from discovering resources available
		  on the Net, and have set the stage for a politics of
		  regulation, repression, and authority. Tech-anxiety, seen
		  as a product of 'containment culture' that has transferred
		  fear of communism to fear of new ideas/technology, mirrors
		  current widespread cultural uncertainty. Net sprawl has
		  also created demands for an authority to determine what is
		  credible in the information overload and establish a
		  foundation for truth to prevent manipulation/deception that
		  could ultimately be destructive to democracy. Exploration
		  of the link between credibility and authority indicates
		  that cyberspace is a potential arena for political battles
		  over what is real involving both progressive causes and
		  those of subversive right-wing groups. However, it is
		  contended that a struggle over credibility and
		  authorization is far superior to regulatory interventions
		  that would privilege some elements at the expense of
		  others. 15 References. J. Lindroth}}

