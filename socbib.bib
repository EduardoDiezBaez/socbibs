	Medline-So = {Prog Transplant 2000 Mar;10(1):33-8; quiz 39-40},
	Medlineref = {20396920},
	Title = {Recommendations for mechanical ventilation during donor care.},
	Abstract = {The organ procurement coordinator usually directs
		  adjustments to the mechanical ventilator during donor care.
		  It is often difficult to achieve optimal oxygen uptake and
		  carbon dioxide removal while avoiding barotrauma or
		  undesirable effects on the cardiac output.
		  Interrelationships among a variety of ventilator parameters
		  must be understood in order to achieve the desired goal of
		  providing the best organs possible. These recommendations
		  review the key ventilator parameters of tidal volume;
		  positive end-expiratory pressure; auto-positive
		  end-expiratory pressure; fraction of inspired oxygen; and
		  flowrate and frequency and their interactions in
		  controlling peak, plateau, and mean and end-expiratory
		  airway pressures.}}

@article{peace97:academ:censor:inter,
	Author = {Peace, A. Graham},
	Date-Added = {2008-10-25},
	Date-Modified = {2008-10-31 11:51:16 -0700},
	Journal = {Journal of Information Ethics},
	Number = {2},
	Optnote = {Fall},
	Pages = {35--47},
	Title = {Academia, Censorship, and the Internet},
	Volume = {6},
	Year = {1997},
	Abstract = {Opinions on the controversy over whether to allow free
		  access to the Internet on university campuses are reviewed.
		  While the Internet has brought unprecedented resources to
		  scholars in the university community, it has also brought
		  objectionable material, eg, pornography, racist propaganda,
		  and instructions for making bombs, to students.
		  Universities are caught in legal and moral conflicts
		  between the First Amendment and free speech and limiting
		  minors' access to pornography. Universities are within
		  their rights to censor morally objectionable material, eg,
		  child pornography, that is not protected by the First
		  Amendment, and to provide access to morally acceptable
		  information protected by the First Amendement, but
		  information in between, especially when minors are
		  involved, is controversial. With input from all
		  stakeholders, universities need to develop procedures to
		  deal with this in-between information, which may involve
		  restricted access to the Internet. 1 Table, 1 Figure, 32
		  References. M. Pflum}}

